{
  "name": "web",
  "private": true,
  "author": {
    "name": "Ron Houben",
    "email": "ron.houben85@gmail.com",
    "url": "https://github.com/RonHouben"
  },
  "scripts": {
    "dev": "npm run dev:tailwind",
    "dev:tailwind": "concurrently \"next dev \" \"npm run tailwind -- --watch\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tailwind": "tailwindcss -i src/styles/globals.css -o src/styles/dist.css",
    "format": "prettier --write \"**/*.{js,ts,tsx,md}\"",
    "postinstall": "npm run tailwind",
    "prisma:db:migrate": "npx prisma migrate dev",
    "prisma:db:seed": "npx prisma db seed",
    "prisma:generate": "npx prisma generate"
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@audio-engine/react": "*",
    "@headlessui/react": "^1.7.7",
    "@heroicons/react": "^2.0.13",
    "@tanstack/react-query": "^4.20.9",
    "@trpc/client": "^10.7.0",
    "@trpc/next": "^10.7.0",
    "@trpc/react-query": "^10.7.0",
    "@trpc/server": "^10.7.0",
    "audio-engine": "*",
    "clsx": "1.2.1",
    "concurrently": "7.6.0",
    "exercise-engine": "*",
    "install": "^0.13.0",
    "music-notes": "*",
    "next": "latest",
    "npm": "^9.2.0",
    "prisma": "^4.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.1",
    "superjson": "^1.12.1",
    "ui": "*",
    "use-reducer-async": "^2.1.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "8.31.0",
    "eslint-config-next": "latest",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwind-scrollbar": "^2.1.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.9.4"
  },
  "prisma": {
    "schema": "../database/prisma/schema.prisma",
    "seed": "ts-node ../database/prisma/seeds/index.ts"
  }
}
