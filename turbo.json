{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "audio-engine#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "web#build": {
      "dependsOn": ["audio-engine#build"],
      "env": ["VERCEL_URL", "PORT", "NODE_ENV"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["db:migrate", "db:seed"],
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    }
  }
}
