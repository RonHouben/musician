{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "audio-engine#build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "web#build": {
      "dependsOn": ["audio-engine#build"],
      "env": ["SOME_OTHER_ENV_VAR"],
      "outputs": [".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
